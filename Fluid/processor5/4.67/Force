FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "4.67";
    object      Force;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    flap
    {
        type            calculated;
        value           nonuniform 0();
    }
    upperWall
    {
        type            calculated;
        value           nonuniform 0();
    }
    lowerWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


